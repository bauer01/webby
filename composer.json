{
	"name": "webby/webby",
	"type": "project",
	"description": "A lightweight flat-file CMS written based on Nette framework",
	"keywords": ["cms","flat-file cms","flat cms","flatfile cms","php","nette"],
	"license": "proprietary",
	"authors": [
		{
			"name": "Bronislav Sedl√°k",
			"homepage": "https://bronislavsedlak.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://toran-proxy.bronislavsedlak.com/repo/private/"
		}
	],
	"require": {
		"php": "^7.1",
		"nette/application": "^2.4",
		"nette/bootstrap": "^2.4",
		"latte/latte": "^2.4",
		"tracy/tracy": "^2.4",
		"unimapper/unimapper": "dev-develop",
		"unimapper/nette": "dev-develop",
		"kriswallsmith/assetic": "^1.4",
		"leafo/scssphp": "^0.6.6",
		"ptachoire/cssembed": "^1.0",
		"oyejorge/less.php": "v1.7.0.10",
		"symfony/console": "^3.2",
		"composer/composer": "^1.4",
		"wikimedia/composer-merge-plugin": "^1.3"
	},
	"autoload": {
		"psr-4": {"Webby\\": "app/Webby"}
	},
	"scripts": {
		"post-create-project-cmd": "bin/system install"
	},
	"minimum-stability": "dev",
	"extra": {
		"merge-plugin": {
			"include": [
				"content/extensions.json"
			]
		}
	},
	"config": {
		"secure-http": false,
		"preferred-install": "dist"
	}
}
